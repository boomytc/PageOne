@model db.view.af_AuditLog

@{
    ViewBag.Title = "审批日志";
}

@section header
{
    <style type="text/css">
    </style>
    <script type="text/javascript">
        //刷新当前页面的数据
        function refleshData(exeCountSql) {
            $("#container").pager("reflesh", exeCountSql);
        }
        //初始化分页
        $(function () {
            $("#container").pager();
        });
    </script>
}

<div class="container"  id="container">
    <div class="pager"></div>
    <div class="showData">
        @{Html.RenderPartial("_LogShowData");}
    </div>
</div>
