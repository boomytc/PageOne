@model db.af_AuditPost

@{
    ViewBag.Title = "职务任职表新增";
    //wzrui 2019-06-16
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <div class="dialog">
        <div class="editor-label">
            @Html.LabelFor(model=>model.postCode)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.postCode)
            @Html.ValidationMessageFor(model => model.postCode)
        </div>

        <div class="editor-label must">
            @Html.LabelFor(model=>model.postName)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.postName)
            @Html.ValidationMessageFor(model => model.postName)
        </div>

        <div class="editor-label must">
            @Html.LabelFor(model=>model.isDeptPost)
        </div>
        <div class="editor-field">
            @Html.DropDownListFor(model => model.isDeptPost,rui.listHelper.bind是否(false))
            @Html.ValidationMessageFor(model => model.isDeptPost)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model=>model.remark)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.remark)
            @Html.ValidationMessageFor(model => model.remark)
        </div>

        <p class="button">
            <a class="billSave opSave" data-close="true">保存</a>
            <a class="opClose">关闭</a>
        </p>
    </div>
}