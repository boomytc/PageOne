@model db.view.af_AuditPostUser
@{
    ViewBag.Title = "用户任职表";
    //wzrui 2019-06-17
}

@section header
{
    <style type="text/css">
    </style>
    <script type="text/javascript">
        //刷新当前页面的数据
        function refleshData(exeCountSql) {
            $("#container").pager("reflesh", exeCountSql);
        }
        //初始化分页
        $(function () {
            $("#container").pager();
        });
    </script>
}
@{ string ctrl = ViewContext.RouteData.Values["controller"].ToString(); }
<div class="container" id="container" data-width="@Model.PageWidth" data-url="/admin/af_AuditPostUser/Select">
    <div class="search">
        <span>用户任职编码：@Html.TextBoxFor(a => a.userCode)</span>
        <span>用户任职名称：@Html.TextBoxFor(a => a.userName)</span>
    </div>
    <div class="toolbar">
        <a class="opSearch">查询</a>
        @Html.ActionLink("批量保存", "batchSave", new { }, new { @class = "listBatchSave opBtn",data_show = rui.privCtl.isPriv(ctrl, rui.privCtl.eOp.BatchSave), data_formid = "form" })
    </div>
    <div class="pager" style="width: 90%; margin-left: auto; margin-right: auto; margin-top: 0px; margin-bottom: 0px;"></div>
    <form id="form">
        <div class="showData">
            @{Html.RenderPartial("_ShowData");}
        </div>
    </form>
</div>