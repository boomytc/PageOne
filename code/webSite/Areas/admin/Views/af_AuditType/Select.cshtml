@model db.view.af_AuditType
@{
    ViewBag.Title = "审批单据类型维护";
}

@section header
{
    <style type="text/css">
    </style>
    <script type="text/javascript">
        //刷新当前页面的数据
        function refleshData(exeCountSql) {
            $("#container").pager("reflesh", exeCountSql);
        }
        //初始化分页
        $(function () {
            $("#container").pager();
        });
    </script>
}
@{ string ctrl = ViewContext.RouteData.Values["controller"].ToString(); }
<div class="container" id="container" data-width="@Model.PageWidth" data-url="/admin/af_AuditType/select">

    <div class="search">
        <span>审批单据名称：@Html.TextBoxFor(a => a.billTypeName)</span>
    </div>
    <div class="toolbar">
        <a class="opSearch">查询</a>
        @Html.ActionLink("新增", "Insert", new { }, new { @class = "showDialogR opInsert", data_show = rui.privCtl.isPriv(ctrl, rui.privCtl.eOp.Insert) })
    </div>

    <div class="pager"></div>
    <div class="showData">
        @{Html.RenderPartial("_ShowData");}
    </div>
</div>
