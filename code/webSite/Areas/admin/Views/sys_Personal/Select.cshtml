@{ string ctrl = ViewContext.RouteData.Values["controller"].ToString(); }
@{
    ViewBag.Title = "定时器";
}
@section header
{
    <style type="text/css">
    </style>
    <script type="text/javascript">
        //刷新当前页面的数据
        function refleshData(exeCountSql) {
            $("#container").pager("reflesh", exeCountSql);
        }
        //初始化分页
        $(function () {
            $("#container").pager();
        });
    </script>
}

<div class="container"  id="container" data-width="@Model.PageWidth" data-url="/admin/sys_Personal/select">
    <div class="showData">
        <table border="1" cellpadding="0" cellspacing="0" id="tableInfo">
            <tr>
                <td>项目名称</td>
                <td>提醒内容</td>
            </tr>
            @foreach (System.Data.DataRow row in (ViewData["personDate"] as System.Data.DataTable).Rows)
            {
                <tr>
                    <td>@row["prjName"]</td>
                    <td>@row["remindContent"]</td>
                </tr>
            }

        </table>
        <input hidden="hidden" type="text" name="sumCount" value="@ViewData["count"]" />
    </div>
</div>